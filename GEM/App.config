<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="PopulationSize" value="100"/>
    <add key="Resume" value="false"/>
    <add key="SavePath" value="D:\Save\GEM"/>
    <!--Save frequency, minutes-->
    <add key="SaveFrequency" value="30"/>
    <!--Do not edit ExperimentID unless you have a very good reason-->
    <add key="ExperimentID" value="0"/>
    <!--Do not edit CurrentGeneration unless you have a very good reason-->
    <add key="CurrentGeneration" value="0"/>
    <!--MutationSeverity should be between 0 and 1-->
    <add key="MutationSeverity" value="0.001"/>
    <!--Number of Cross-validations to do per dataset-->
    <add key="NumCrossValids" value="5"/>
    <!--How big part of the population gets kept during the elitist selection-->
    <add key="EliteRatio" value="0.05"/>
    <!--How many fitness values to keep for the stop criterion-->
    <add key="PastFitnessCount" value="10"/>
    <!--Minimum of the 1st derivative of the fitness curve for stop crit.-->
    <add key="MinFitnessDerivative" value="0.00001"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
